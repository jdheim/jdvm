{
  "__COMMENT__ More Information": "https://github.com/mozilla/policy-templates/tree/master#readme",
  "policies": {
    "DisableAppUpdate": true,
    "DisableFirefoxStudies": true,
    "DisableTelemetry": true,
    "DisableFeedbackCommands": true,
    "DisablePocket": true,
    "DisableSetDesktopBackground": true,
    "DontCheckDefaultBrowser": true,
    "DefaultDownloadDirectory": "${home}",
    "DownloadDirectory": "${home}",
    "PromptForDownloadLocation": true,
    "DisplayBookmarksToolbar": "always",
    "NoDefaultBookmarks": true,
    "ManagedBookmarks": [
      {
        "toplevel_name": "JDVM"
      },
      {
        "url": "https://github.com/jdheim/jdvm",
        "name": "Home"
      },
      {
        "url": "https://github.com/jdheim/jdvm/discussions",
        "name": "Discussions"
      },
      {
        "url": "https://github.com/jdheim/jdvm/issues",
        "name": "Support"
      }
    ],
    "Bookmarks": [
      {
        "Title": "DEV.java",
        "URL": "https://dev.java",
        "Placement": "toolbar"
      },
      {
        "Title": "localhost:80",
        "URL": "http://localhost",
        "Placement": "toolbar"
      },
      {
        "Title": "localhost:443",
        "URL": "https://localhost",
        "Placement": "toolbar"
      }
    ],
    "ExtensionSettings": {
      "uBlock0@raymondhill.net": {
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi"
      }
    },
    "SearchEngines": {
      "Add": [
        {
          "Name": "Brave",
          "URLTemplate": "https://search.brave.com/search?q={searchTerms}",
          "IconURL": "data:image/svg+xml;base64,",
          "Alias": "@brave"
        }
      ],
      "Remove": [
        "Bing",
        "eBay",
        "Wikipedia (en)"
      ],
      "Default": "DuckDuckGo"
    },
    "SupportMenu": {
      "Title": "JDVM Support",
      "URL": "https://github.com/jdheim/jdvm/issues",
      "AccessKey": "S"
    }
  }
}
