  export PATH="/opt/krew/bin:${PATH}"
fi
if [[ ":${PATH}:" != *":${HOME}/.krew/bin:"* ]]; then
  export PATH="${HOME}/.krew/bin:${PATH}"
fi
if [[ ":${PATH}:" != *":${HOME}/.local/bin:"* ]]; then
  export PATH="${HOME}/.local/bin:${PATH}"
fi

export GOPATH="${HOME}/.go"

